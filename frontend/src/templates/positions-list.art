{{each data}}
<tr>
    <td>{{$index+1}}</td>
    <td><img src="http://placehold.it/50x50" width='50' height='50' alt=""></td>
    <td>{{$value.company}}</td>
    <td>{{$value.position}}</td>
    <td>{{$value.location}}</td>
    <td>{{$value.salary}}</td>
    <td>{{$value.createTime}}</td>
    <td>
        <button data-id="{{$value._id}}" class="btn btn-primary update">Update</button>
        <button data-id="{{$value._id}}" class="btn btn-danger remove">Delete</button>
    </td>
</tr>
{{/each}}